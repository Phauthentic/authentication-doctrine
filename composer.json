{
    "name": "phauthentic/authentication-doctrine",
    "require": {
        "php": ">=7.1",
        "phauthentic/authentication": "dev-master",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/dbunit": "^4.0",
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phauthentic\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phauthentic\\Authentication\\Test\\": [
                "tests/",
                "vendor/phauthentic/authentication/tests/"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
